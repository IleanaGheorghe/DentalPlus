<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#85CFCE"
    tools:context=".DoctorInterface.ChangePersonalInfoFragment">


    <TextView
        android:id="@+id/tvNume"
        android:layout_width="98dp"
        android:layout_height="31dp"
        android:text="Nume:"
        android:background="@drawable/background_login"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.068" />

    <EditText
        android:id="@+id/etNume"
        android:layout_width="300dp"
        android:layout_height="45dp"
        android:layout_marginStart="44dp"
        android:background="@drawable/background_login"
        android:ems="10"
        android:hint="Nume"
        android:inputType="textPersonName"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvNume"
        app:layout_constraintVertical_bias="0.024" />

    <TextView
        android:id="@+id/tvPrenume"
        android:layout_width="113dp"
        android:layout_height="35dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/background_login"
        android:text="Prenume:"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etNume"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/etPRenume"
        android:layout_width="300dp"
        android:layout_height="45dp"
        android:layout_marginStart="36dp"
        android:ems="10"
        android:hint="Prenume"
        android:inputType="textPersonName"
        android:background="@drawable/background_login"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPrenume"
        app:layout_constraintVertical_bias="0.028" />

    <TextView
        android:id="@+id/tvEmail"
        android:layout_width="116dp"
        android:layout_height="38dp"
        android:text="Email:"
        android:background="@drawable/background_login"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etPRenume"
        app:layout_constraintVertical_bias="0.051" />

    <EditText
        android:id="@+id/etEmail"
        android:layout_width="300dp"
        android:layout_height="48dp"
        android:layout_marginStart="36dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textPersonName"
        android:background="@drawable/background_login"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvEmail"
        app:layout_constraintVertical_bias="0.033" />

    <TextView
        android:id="@+id/tvPhone"
        android:layout_width="114dp"
        android:layout_height="39dp"
        android:text="Telefon:"
        android:background="@drawable/background_login"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etEmail"
        app:layout_constraintVertical_bias="0.053" />

    <EditText
        android:id="@+id/etTelefon"
        android:layout_width="300dp"
        android:layout_height="48dp"
        android:layout_marginStart="36dp"
        android:ems="10"
        android:hint="Telefon"
        android:inputType="textPersonName"
        android:background="@drawable/background_login"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPhone"
        app:layout_constraintVertical_bias="0.075" />

    <Button
        android:id="@+id/buttonSave"
        android:layout_width="117dp"
        android:layout_height="53dp"
        android:background="@drawable/background_signup"
        android:clickable="true"
        android:focusable="true"
        android:padding="16dp"
        android:text="Salvare"
        android:textAllCaps="false"
        android:textColor="#FFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.863"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etTelefon"
        app:layout_constraintVertical_bias="0.194" />
</androidx.constraintlayout.widget.ConstraintLayout>